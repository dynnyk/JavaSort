import java.util.Arrays;

/**
 * Created by Petro on 30.10.2015.
 */
public class test{
    public static void main(String[] args) {
        Integer[] arr = new Integer[100];
        Integer[] arr2 = new Integer[12];

        for (int i = 0; i < arr.length; i++) {
            arr[i] = (int)(Math.random()*100);
        }
        print(arr);
        arr2 = arr;
        //Standart sorting
        long a1 = System.nanoTime();
        Arrays.sort(arr2);
        long a2 = System.nanoTime();

        //Insertion sorting
        long a3 = System.nanoTime();
        arr = mySort(arr);
        long a4 = System.nanoTime();
        print(arr);
        System.out.println("time Insertion_Sort = " + (a4 - a3));
        System.out.println("time Standart Sort= " + (a2 - a1));



    }
    static Integer[] mySort(Integer[] a){
        int key, i, temp;
        for (int j = 1; j < a.length - 1; j++) {
            key = a[j];
            i = j - 1;
            while (i>=0 && a[i] > key){
                temp = a[i+1];
                a[i+1] = a[i];
                a[i] = temp;
                i= i - 1;
            }
        }
        return a;
    }
    static void print(Integer[] a){
        System.out.print(" [");
        for (int i = 0; i < a.length; i++) {
            System.out.print(a[i] + ",  ");
        }
        System.out.println("]");
    }
    static void addPrintArr(int[][] nums){
        int temp = 10;
        for (int i = 0; i < nums.length; i++) {
            for (int j = 0; j < nums[0].length; j++) {
                nums[i][j] = temp;
                temp+=10;
                System.out.print(nums[i][j] + ", ");
            }
            System.out.println();
        }
    }
}

